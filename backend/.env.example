# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key

# Gemini API (free tier: 1500 requests/day)
# Used for: Postdoc agent, Reviewers agent (FREE)
GEMINI_API_KEY=your_gemini_api_key

# Railway deployment
PORT=3000
BASE_URL=https://your-app.up.railway.app

# ========================================
# AI-Researcher Agent Laboratory Configuration
# ========================================

# OpenRouter API (RECOMMENDED for Agent Laboratory)
# Get your key at: https://openrouter.ai/keys
# Provides access to Claude Opus 4.5, Claude Sonnet 4.5, GPT-5 Codex, and more
OPENROUTER_API_KEY=your_openrouter_key

# Gemini API (FREE tier: 1500 requests/day)
# Get your key at: https://aistudio.google.com/apikey
# Used for free fallback models
GEMINI_API_KEY=your_gemini_api_key

# OpenAI API (optional alternative)
# Get your key at: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_key

# Anthropic API (optional, for direct Claude access)
ANTHROPIC_API_KEY=your_anthropic_key

# DeepSeek API (optional alternative)
DEEPSEEK_API_KEY=your_deepseek_key

# Default completion model (when not using per-agent config)
COMPLETION_MODEL=gemini-2.0-flash-exp

# Enable/disable AI Researcher features
AI_RESEARCHER_ENABLED=true

