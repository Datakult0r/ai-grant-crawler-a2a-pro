# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key

# Gemini API (free tier: 1500 requests/day)
# Used for: Postdoc agent, Reviewers agent (FREE)
GEMINI_API_KEY=your_gemini_api_key

# Railway deployment
PORT=3000
BASE_URL=https://your-app.up.railway.app

# ========================================
# AI-Researcher Agent Laboratory Configuration
# ========================================

# OpenRouter API (for multi-provider model routing)
# Get your key at: https://openrouter.ai/keys
# Used for: Claude Opus 4.5, Claude Sonnet 4.5, GPT-5 Codex
OPENROUTER_API_KEY=your_openrouter_key

# OpenAI API (optional, for direct OpenAI access)
OPENAI_API_KEY=your_openai_key

# Anthropic API (optional, for direct Claude access)
ANTHROPIC_API_KEY=your_anthropic_key

# Default completion model (when not using per-agent config)
COMPLETION_MODEL=gemini-3-pro

