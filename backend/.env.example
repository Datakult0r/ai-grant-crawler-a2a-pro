# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key

# Gemini API (free tier: 1500 requests/day)
# Used for: Postdoc agent, Reviewers agent (FREE)
GEMINI_API_KEY=your_gemini_api_key

# Railway deployment
PORT=3000
BASE_URL=https://your-app.up.railway.app

# ========================================
# AI-Researcher Agent Laboratory Configuration
# ========================================

# OpenAI API (REQUIRED for Agent Laboratory)
# Get your key at: https://platform.openai.com/api-keys
# Used by the Agent Laboratory research system
OPENAI_API_KEY=your_openai_key

# OpenRouter API (optional, for multi-provider model routing)
# Get your key at: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_key

# Anthropic API (optional, for direct Claude access)
ANTHROPIC_API_KEY=your_anthropic_key

# DeepSeek API (optional alternative)
DEEPSEEK_API_KEY=your_deepseek_key

# Default completion model (when not using per-agent config)
COMPLETION_MODEL=gemini-2.0-flash-exp

# Enable/disable AI Researcher features
AI_RESEARCHER_ENABLED=true

# ========================================
# Agent Laboratory Model Configuration
# ========================================
# Per-agent models are configured in ai-researcher/agent_models.py
# Default: Uses gpt-4o-mini for fast, cost-effective research
# You can upgrade to gpt-4o or other models in the config
